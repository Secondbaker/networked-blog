
<p id="notice"><%= notice %></p>
<div id='app' class='bg-black '>

    <%= react_component 'Graph', {message: 'test', nodes: @blog_posts.all} %>
    <div class='panelWrapper app-body'>


        <div id='text-posts' class='text-block-container w-4/6 mx-auto grid grid-cols-1 gap-10' data-selected='none'>
            <% @blog_posts.each do |blog_post|%>
                <%= render partial: 'blog_post', locals: { blog_post: blog_post }%>
            <% end %>
        </div>
    </div>

</div>

<%= link_to 'New Blog Post', new_blog_post_path %>
