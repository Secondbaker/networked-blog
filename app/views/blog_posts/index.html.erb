<%= include_gon %>

<p id="notice"><%= notice %></p>
<div id='app'>

    <div id='cy'></div>
    <div class='panelWrapper app-body'>


        <div id='text-posts' class='text-block-container' data-selected='none'>
            <% @blog_posts.each do |blog_post|%>
            <%= blog_posts_url %>
            <%= render partial: 'blog_post', locals: { blog_post: blog_post }%>
            <% end %>
        </div>
        <div id='cy' class='panel' data-enter='slideleft' data-exit='slideright'></div>
    </div>

</div>

<%= link_to 'New Blog Post', new_blog_post_path %>
<%= javascript_pack_tag 'graph'%>